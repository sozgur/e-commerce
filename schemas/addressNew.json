      "type": "string",
      "minLength": 1,
      "maxLength": 50
    },
    "firstName": {
      "type": "string",
      "minLength": 1,
      "maxLength": 50
    },
    "lastName": {
      "type": "string",
      "minLength": 1,
      "maxLength": 50
    },
    "address1": {
      "type": "string",
